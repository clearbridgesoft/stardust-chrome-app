{
	"name": "Stardust Chrome App",
	"description": "A Chrome App port for Stardust Mobile UI",
	"version": "0.1",
	"manifest_version": 2,
	"app": {
		"background": {
			"scripts": ["main.js"]
		}
	},
	"permissions": [
		"storage",
		"http://*/"
	],
	"offline_enabled": false,
	"icons": {
		"16": "images/appStardust.png",
		"128": "images/splashStardust.png"
	}
}